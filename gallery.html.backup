    // Function to create gallery sections with group cards
    function createGallery() {
      const container = document.getElementById('gallery-container');
      
      for (const [category, images] of Object.entries(galleryData)) {
        // Create category section
        const categorySection = document.createElement('div');
        categorySection.className = 'gallery-category';
        
        // Add category title
        const categoryTitle = document.createElement('h2');
        categoryTitle.className = 'gallery-category-title';
        categoryTitle.textContent = category;
        categorySection.appendChild(categoryTitle);
        
        // Create grid for group cards
        const grid = document.createElement('div');
        grid.className = 'gallery-grid';
        
        // Create a single group card for this category
        const galleryCard = document.createElement('div');
        galleryCard.className = 'gallery-card';
        galleryCard.setAttribute('data-aos', 'fade-up');
        
        // Create card content
        const cardContent = document.createElement('div');
        cardContent.className = 'gallery-card-content';
        
        // Add cover image (first image in the group)
        const coverImg = document.createElement('img');
        coverImg.src = images[0];
        coverImg.alt = `${category} - Cover Image`;
        coverImg.loading = 'lazy';
        coverImg.className = 'gallery-card-cover';
        
        // Add image count badge
        const countBadge = document.createElement('div');
        countBadge.className = 'image-count-badge';
        countBadge.textContent = `${images.length} Photos`;
        
        // Add card overlay
        const overlay = document.createElement('div');
        overlay.className = 'gallery-overlay';
        
        // Add card title
        const cardTitle = document.createElement('div');
        cardTitle.className = 'gallery-card-title';
        cardTitle.textContent = category;
        
        // Add view gallery button
        const viewBtn = document.createElement('button');
        viewBtn.className = 'view-gallery-btn';
        viewBtn.innerHTML = '<i class="bi bi-images"></i> View Gallery';
        
        // Add zoom indicator
        const zoomIndicator = document.createElement('div');
        zoomIndicator.className = 'zoom-indicator';
        zoomIndicator.innerHTML = '<i class="bi bi-zoom-in"></i> Zoom available';
        
        // Append elements to card
        overlay.appendChild(cardTitle);
        overlay.appendChild(viewBtn);
        cardContent.appendChild(coverImg);
        cardContent.appendChild(countBadge);
        cardContent.appendChild(zoomIndicator);
        cardContent.appendChild(overlay);
        galleryCard.appendChild(cardContent);
        
        // Create thumbnail preview
        const thumbnailPreview = document.createElement('div');
        thumbnailPreview.className = 'thumbnail-preview';
        
        // Add up to 4 thumbnail images
        const previewCount = Math.min(4, images.length);
        for (let i = 0; i < previewCount; i++) {
          const thumb = document.createElement('div');
          thumb.className = 'thumbnail';
          const thumbImg = document.createElement('img');
          thumbImg.src = images[i];
          thumbImg.alt = `${category} - Thumbnail ${i + 1}`;
          thumbImg.loading = 'lazy';
          thumb.appendChild(thumbImg);
          thumbnailPreview.appendChild(thumb);
        }
        
        galleryCard.appendChild(thumbnailPreview);
        
        // Add click event to open all images in lightbox
        galleryCard.addEventListener('click', () => {
          openGroupLightbox(images, category);
        });
        
        grid.appendChild(galleryCard);
        categorySection.appendChild(grid);
        container.appendChild(categorySection);
      }
    }

    // Function to open lightbox
    function openLightbox(imagePath, caption) {
      const lightbox = GLightbox({
        elements: [
          {
            href: imagePath,
            title: caption
          }
        ],
        zoomable: true,
        draggable: true,
        closeOnOutsideClick: true,
        slideEffect: 'fade',
        height: '90vh',
        width: '90vw'
      });
      lightbox.open();
    }
    
    // Function to open lightbox gallery for a group of images
    function openGroupLightbox(images, categoryTitle) {
      const elements = images.map((imagePath, index) => {
        return {
          href: imagePath,
          title: `${categoryTitle} - Image ${index + 1} of ${images.length}`
        };
      });
      
      const lightbox = GLightbox({
        elements: elements,
        touchNavigation: true,
        loop: true,
        autoplayVideos: false,
        zoomable: true,
        draggable: true,
        closeOnOutsideClick: true,
        slideEffect: 'fade',
        height: '90vh',
        width: '90vw',
        plyr: {
          css: 'https://cdn.plyr.io/3.6.8/plyr.css',
          js: 'https://cdn.plyr.io/3.6.8/plyr.js',
          config: {
            ratio: '16:9',
            fullscreen: { enabled: true, iosNative: true }
          }
        }
      });
      
      lightbox.open();
    }

    // Scroll to top function
    function scrollToTop() {
      window.scrollTo({
        top: 0,
        behavior: 'smooth'
      });
    }

    // Initialize gallery when page loads
    document.addEventListener('DOMContentLoaded', () => {
      createGallery();
      
      // Show/hide back to top button
      window.addEventListener('scroll', () => {
        const backToTopBtn = document.querySelector('.back-to-top');
        if (backToTopBtn) {
          if (window.scrollY > 300) {
            backToTopBtn.style.opacity = '1';
            backToTopBtn.style.visibility = 'visible';
          } else {
            backToTopBtn.style.opacity = '0';
            backToTopBtn.style.visibility = 'hidden';
          }
        }
      });
      
      // Initialize AOS
      AOS.init({
        duration: 800,
        easing: 'ease-in-out',
        once: true
      });
    });
  </script>

